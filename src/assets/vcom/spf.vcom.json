{
    "name": "spf",
    "var": "SpfVcom",
    "prefix": "sv",

    "version": {
        "@": "1.0.0",
        "latest": "1.0.0"
    },

    "required": [
        "icon-*", "button-*","layout-*", "tabbar-*", "win-*"
    ],
    "loadmode": {
        "basic": [
            "navbar", "desktop-*"
        ]
    },

    "vue": {

        "ui": {
            "element-ui": {
                "file": "spf/assets/cdn/element-ui.cdn.json",
                "params": {
                    "ver": "@"
                },
                "export": {
                    "ui.js": {
                        "export": "js",
                        "file": "default"
                    },
                    "esm-ui.js": {
                        "export": "js",
                        "file": "esm-browser"
                    },
                    "ui.css": {
                        "light": {
                            "export": "css",
                            "file": "light"
                        },
                        "dark": {
                            "export": "css",
                            "file": "dark"
                        }
                    }
                }
            }
        }
    },

    "theme": {
        "enable": true,
        "file": "spf/assets/theme/spf",
        "params": {
            "mode": "light"
        }
    },

    "iconset": [
        "spf/assets/icon/md-round",
        "spf/assets/icon/md-sharp",
        "spf/assets/icon/md-fill",
        "spf/assets/icon/spinner",
        "spf/assets/icon/spf"
    ]
    
}