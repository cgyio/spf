{
    "name": "vue",
    "var": "Vue",

    "enableVersion": true,
    "version": {
        "@": "2.7.16",
        "latest": "3.5.21"
    },

    "enableCache": true,
    "cachePath": "cache",

    "ext": [
        "js"
    ],

    "root": "https://cdnjs.cloudflare.com/ajax/libs/vue",
    
    "content": {
        "2.7.16": {
            "js": {
                "dev-esm-browser": {
                    "comment": "开发环境，esm导入，用于浏览器端",
                    "file": "vue.common.dev.min.js",
                    "params": {
                        "esm": true
                    }
                },
                "default": {
                    "comment": "生产环境，esm导入，用于浏览器端",
                    "file": "vue.esm.browser.min.js",
                    "params": {
                        "esm": true
                    }
                },
                "dev-browser": {
                    "file": "vue.common.dev.min.js",
                    "params": {
                        "esm": false
                    },
                    "fix": [
                        "ModuleNotDefined"
                    ]
                },
                "browser": {
                    "file": "vue.common.prod.min.js",
                    "params": {
                        "esm": false
                    }
                }
            }
        },
    
        "3.5.21": {
            "js": {
                "dev-esm-browser": {
                    "file": "vue.esm-browser.min.js",
                    "params": {
                        "esm": true
                    }
                },
                "default": {
                    "file": "vue.esm-browser.prod.min.js",
                    "params": {
                        "esm": true
                    }
                },
                "dev-browser": {
                    "file": "vue.global.min.js",
                    "params": {
                        "esm": false
                    }
                },
                "browser": {
                    "file": "vue.global.prod.min.js",
                    "params": {
                        "esm": false
                    }
                }
            }
        }
    }
}